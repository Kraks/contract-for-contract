{
  "name": "consol",
  "type": "module",
  "version": "0.0.1",
  "description": "Implement higher-order temporal behavioral contracts in Solidity",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@8.19.2",
  "keywords": [],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "consol": "bin/consol.js"
  },
  "scripts": {
    "gen:parser": "zsh scripts/gen-parser.zsh",
    "build": "npm run clean && tsc --build",
    "clean": "rm -rf dist/*",
    "lint": "eslint ./src ./test --ext=ts,js,cjs,mjs --no-error-on-unmatched-pattern",
    "lint:fix": "eslint ./src ./test --ext=ts,js,cjs,mjs --no-error-on-unmatched-pattern --fix",
    "format": "prettier --no-error-on-unmatched-pattern --write './**/*.{ts,js,cjs,mjs,json,jsonc}'",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "antlr4": "^4.12.0",
    "solc-typed-ast": "^14.0.0",
    "type-fest": "^3.10.0"
  }
}
